{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenAi": {
    "type": "azure",
    "api_version": "2023-07-01-preview",
    "endpoint": "https://<your-service>.openai.azure.com/",
    "deployment": "gpt-chat",
    "embedding_deployment": "text-embedding"
  },
  "CosmosDb": {
    "Endpoint": "https://<your-service>.documents.azure.com:443/",
    "databaseName": "chatdatabase",
    "containerName": "customers"
  },
  "AzureAISearch": {
    "Endpoint": "https://<your-service>.search.windows.net",
    "index_name": "azure-support"
  },
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=00000000-0000-0000-0000-000000000000;IngestionEndpoint=https://<your-service>.applicationinsights.azure.com/"
  }
}
